<section class="login py-5 border-top-1">
        <div class="container">
            <div class="row justify-content-center">
                <div class="col-lg-5 col-md-8 align-item-center">
                    <div class="border border">
                        <h3 class="bg-gray p-4">매장 정보 수정</h3>
                        <form id="store_mod" action="/store_mod/<%=row.ID%>" method="post" enctype="multipart/form-data">
                            <fieldset class="p-4">
                                매장 이름<input type="text" class="border p-2 w-100 my-2" name="name" id="name" size="12" maxLength="12" value=<%=row.NAME%> required>
                                배달 시간<input type="text" class="border p-2 w-100 my-2" name="dtime" id="dtime" size="3" maxLength="3" value=<%=row.DELIVERY_TIME%> required>분
                                최소 주문 금액<input type="text" class="border p-2 w-100 my-2" name="price" id="price" size="6" maxLength="6" value=<%=row.PRICE_LIMIT%> required>원
                                오픈 시간<input type="time" class="border p-2 w-100 my-2" name="open" id="open" min="00:00:00" max="23:59:59" value=<%=row.UPTIME%>>
                                마감 시간<input type="time" class="border p-2 w-100 my-2" name="close" id="close" min="00:00:00" max="23:59:59" value=<%=row.CLOSETIME%>>
                                전화번호<input type="hidden" class="border p-2 w-100 my-2" name="tel" id="tel">
                <select name="tel1" id="tel1">
                    <option value="010">010</option>
                    <option value="011">011</option>
                    <option value="016">016</option>
                    <option value="017">017</option>
                    <option value="018">018</option>
                    <option value="019">019</option>
                </select>
                -
                <input type="text" class="border p-2 w-70 my-2" name="tel2" id="tel2" size="4" maxLength="4" <%if(row.PHONE){%>value="<%=row.PHONE[4]%><%=row.PHONE[5]%><%=row.PHONE[6]%><%=row.PHONE[7]%>" <%}%> required>
                -
                <input type="text" class="border p-2 w-70 my-2" name="tel3" id="tel3" size="4" maxLength="4" <%if(row.PHONE){%>value="<%=row.PHONE[9]%><%=row.PHONE[10]%><%=row.PHONE[11]%><%=row.PHONE[12]%>"  <%}%> required>
                                
                <br>
                이미지 업로드<br><input type="file" name="filename" value=<%=row.STORE_IMG%>><br>
                <input type="button" style="float:right;" class="py-3 px-4 bg-primary text-white border-0 rounded font-weight-bold" id="btnRtn" name="return" value="돌아가기">
                <input type="button" style="float:right; margin-right: 5px" class="py-3 px-4 bg-primary text-white border-0 rounded font-weight-bold" id="btnJoin" name="join" value="수정" >
                
                            </fieldset>
                        </form>
                    </div>
                </div>
            </div>
        </div>
    </section>
